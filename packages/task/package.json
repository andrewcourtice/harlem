{
    "name": "@harlem/task",
    "amdName": "harlemTask",
    "version": "2.4.0-beta.8",
    "license": "MIT",
    "author": "Andrew Courtice <andrewcourtice@users.noreply.github.com>",
    "description": "Harlem task package",
    "homepage": "https://harlemjs.com",
    "main": "index.js",
    "module": "dist/index.esm-bundler.js",
    "types": "dist/index.d.ts",
    "unpkg": "dist/index.global.js",
    "jsdelivr": "dist/index.global.js",
    "sideEffects": false,
    "files": [
        "dist",
        "index.js",
        "index.mjs"
    ],
    "exports": {
        ".": {
            "import": {
                "node": "./index.mjs",
                "default": "./dist/index.esm-bundler.js"
            },
            "require": "./index.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/*": "./dist/*",
        "./package.json": "./package.json"
    },
    "keywords": [
        "vue",
        "state",
        "harlem",
        "task"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/andrewcourtice/harlem.git",
        "directory": "packages/task"
    },
    "bugs": {
        "url": "https://github.com/andrewcourtice/harlem/issues"
    },
    "scripts": {
        "build": "node ./build.mjs",
        "prepublishOnly": "yarn build"
    }
}
