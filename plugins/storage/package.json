{
    "name": "@harlem/plugin-storage",
    "amdName": "harlemStorage",
    "version": "1.1.3",
    "license": "MIT",
    "author": "Andrew Courtice <andrewcourtice@users.noreply.github.com>",
    "description": "The official storage plugin for Harlem",
    "homepage": "https://harlemjs.com",
    "main": "dist/harlem-storage.js",
    "module": "dist/harlem-storage.esm.js",
    "umd:main": "dist/harlem-storage.umd.js",
    "unpkg": "dist/harlem-storage.min.js",
    "types": "dist/index.d.ts",
    "source": "src/index.ts",
    "exports": {
        ".": {
            "require": "./dist/harlem-storage.js",
            "umd": "./dist/harlem-storage.umd.js",
            "browser": "./dist/harlem-storage.esm.js",
            "import": "./dist/harlem-storage.esm.js"
        }
    },
    "keywords": [
        "vue",
        "state",
        "harlem",
        "plugin",
        "storage"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/andrewcourtice/harlem.git",
        "directory": "plugins/storage"
    },
    "bugs": {
        "url": "https://github.com/andrewcourtice/harlem/issues"
    },
    "scripts": {
        "dev": "microbundle watch --raw",
        "build": "yarn build:core && yarn build:min",
        "build:core": "microbundle build --raw",
        "build:min": "microbundle build --raw -f iife src/index.ts -o dist/harlem-storage.min.js",
        "prepublish": "yarn build"
    },
    "peerDependencies": {
        "@harlem/core": "^1.1.2"
    },
    "devDependencies": {
        "@harlem/core": "^1.1.3"
    }
}
